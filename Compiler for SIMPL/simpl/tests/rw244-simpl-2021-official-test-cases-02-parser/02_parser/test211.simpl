program test211
begin
    integer i, j;
    boolean k;

    i <- 0;
    j <- 5;
    while i < 5 do
        while j > 0 do
            j <- j - 1;
            k <- j = i;
            write i;
            write j;
            write k
        end;
        i <- i + 1
    end;
    write i;
    write j
end
